{
  "name": "branch",
  "version": "1.0.0",
  "description": "Backend Services Branch project",
  "author": "Cristian Nieto (cristianandresnieto@gmail.com)",
  "private": true,
  "scripts": {
    "start": "node ./dist/index",
    "dev": "nodemon",
    "build": "tsc",
    "pretest": "npx rimraf database.sqlite3 && cross-env NODE_ENV=test npx sequelize-cli db:migrate && cross-env NODE_ENV=test npx sequelize-cli db:seed:all",
    "test": "npm run build && cross-env NODE_ENV=test nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ."
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node src/index.ts"
  },
  "engines": {
    "node": ">10.16.0"
  },
  "dependencies": {
    "aws-sdk": "^2.629.0",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf-sync": "^4.0.1",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-joi-validation": "^5.0.1",
    "express-session": "^1.17.3",
    "firebase-admin": "11.5.0",
    "firebase-functions": "^3.3.0",
    "follow-redirects": "^1.15.4",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.4.0",
    "joi": "^17.11.0",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.2",
    "sequelize": "^6.29.0",
    "sequelize-paginate": "^1.1.6",
    "socket.io": "4.4.1",
    "sqlite3": "^5.1.5",
    "twilio": "^3.39.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.8",
    "@types/cls-hooked": "^4.3.3",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.9",
    "@types/follow-redirects": "^1.14.1",
    "@types/helmet": "^4.0.0",
    "@types/mocha": "^9.1.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^16.0.0",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.6.2",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-plugin-jsdoc": "^35.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-security": "^1.7.1",
    "mocha": "^9.2.2",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-multi-reporters": "^1.5.1",
    "nyc": "^15.1.0",
    "sequelize-cli": "^5.5.1",
    "sinon": "^13.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
